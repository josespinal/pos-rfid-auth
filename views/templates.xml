<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Include JavaScript and CSS assets for POS -->
        <template id="assets_frontend" inherit_id="point_of_sale.assets">
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/pos_rfid_auth/static/src/js/rfid_reader.js"></script>
                <script type="text/javascript" src="/pos_rfid_auth/static/src/js/screen_locker.js"></script>
                <script type="text/javascript" src="/pos_rfid_auth/static/src/js/rfid_auth_popup.js"></script>
                <script type="text/javascript" src="/pos_rfid_auth/static/src/js/pos_rfid_auth.js"></script>
            </xpath>
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" href="/pos_rfid_auth/static/src/css/pos_rfid_auth.css"/>
            </xpath>
        </template>
    </data>
</odoo> 